{
  "manifest_version": 2,
  "name": "img changer",
  "version": "1.0",
  "description": "This extension changes the image of the web page",
  "web_accessible_resources":[
    "puppies/*.jpg"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["puppies.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}
